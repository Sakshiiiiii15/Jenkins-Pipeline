    agent any
    stages{
        stage("Build"){
            steps{
                echo "Building..."
            }
            post{
                success{
                    mail to: "sakshiranan22@gmail.com",
                    subject: "Build Status Email",
                    body: " Build was successfull"
                }
            }
     stages{
        stage("Complete"){
            steps{
                echo "Completed."
            }
        }
    }
}
}
